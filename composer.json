{
  "name": "alirezaabdolmaleki/vip-sms",
  "type": "library",
  "description": "A simple API extension for send text message By Easy Sms",
  "keywords": [
    "alirezaabdolmaleki",
    "VipSms"
  ],
  "support": {
    "issues": "https://github.com/alirezaabdolmaleki/VipSms/issues",
    "source": "https://github.com/alirezaabdolmaleki/VipSms"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alireza Abdolmaleki",
      "email": "alaabibest@gmail.com",
      "homepage": "https://alireza-abdolmaleki.ir"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.4",
    "illuminate/support": "*",
    "illuminate/notifications": "*",
    "guzzlehttp/guzzle": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Alirezaabdolmaleki\\VipSms\\": "src/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alirezaabdolmaleki\\VipSms\\VipSmsServiceProvider"
      ],
        "aliases": {
            "RayganSms": "Alirezaabdolmaleki\\VipSms\\Facades"
        }
    }
  }
}
